{
  "extends": [
    ":npm",
    ":docker",
    ":separateMajorReleases",
    ":combinePatchMinorReleases",
    ":ignoreUnstable",
    ":prImmediately",
    ":semanticCommitsDisabled",
    ":automergeDisabled",
    ":ignoreModulesAndTests",
    ":pinAllExceptPeerDependencies",
    "group:monorepos",
    "group:recommended",
    "helpers:disableTypesNodeMajor"
  ],
  "timezone": "Europe/London",
  "labels": [
    "dependencies"
  ],
  "branchPrefix": "renovate-",
  "rangeStrategy": "pin",
  "dependencyDashboard": true,
  "dependencyDashboardApproval": true,
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "@aws-sdk/"
      ],
      "groupName": "@aws-sdk",
      "groupSlug": "aws-sdk"
    },
    {
      "matchDatasources": [
        "terraform-module",
        "terraform-provider"
      ],
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "terraform: bump all non-major dependencies",
      "groupSlug": "terraform-prod-dependencies",
      "dependencyDashboardApproval": false
    },
    {
      "matchDatasources": [
        "npm"
      ],
      "matchPackagePatterns": [
        "*"
      ],
      "depTypeList": [
        "dependencies"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "npm: bump all non-major dependencies",
      "groupSlug": "npm-prod-dependencies",
      "dependencyDashboardApproval": false
    },
    {
      "matchDatasources": [
        "npm"
      ],
      "matchPackagePatterns": [
        "*"
      ],
      "depTypeList": [
        "devDependencies"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "npm: bump all non-major devDependencies",
      "groupSlug": "npm-dev-dependencies",
      "dependencyDashboardApproval": false
    }
  ]
}



